    "@blueprintjs/docs-theme": "^3.0.1",
    "@blueprintjs/table": "^3.1.1",
    "@types/expect": "^1.20.3",
    "@types/fetch-mock": "^6.0.3",
    "@types/react-redux": "^6.0.6",
    "@types/react-table": "^6.7.12",
    "@types/redux-logger": "^3.0.6",
    "@types/redux-mock-store": "^1.0.0",
    "cross-fetch": "^2.2.2",
    "fetch-mock": "^6.5.2",
    "jest": "^23.5.0",
    "jest-css-modules": "^1.1.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-pagination-table": "^2.0.2",
    "react-redux": "^5.0.7",
    "react-scripts-ts": "2.17.0",
    "react-table": "^6.8.6",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "redux-thunk-actions": "^1.2.1",
    "ts-jest": "^23.1.4"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "jest",
    "compile": "tsc"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.0.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "typescript": "^3.0.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules",
      "\\.(css|jpg|png|svg)$": "<rootDir>/src/empty-module.js"
    }
  }
}
